{
    "name": "Tab Matcher",
    "description": "Automatically group your tabs!",
    "version": "1.0",
    "manifest_version": 3,
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "tabGroups"
    ]
}
